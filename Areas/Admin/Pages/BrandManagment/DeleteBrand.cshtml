@page
@model AssetProject.Areas.Admin.Pages.BrandManagment.DeleteBrandModel
@{
}

<div class="slim-mainpanel">
      <div class="container">
        <div class="slim-pageheader">
            <ol class="breadcrumb slim-breadcrumb">
                <li class="breadcrumb-item"><a href="#">@sharedResource["Home"]</a></li>
                <li class="breadcrumb-item"><a href="#">@sharedResource["SETUP"]</a></li>
                <li class="breadcrumb-item active" aria-current="page">@sharedResource["DeleteBrand"]</li>
            </ol>
          <h6 class="slim-pagetitle">@sharedResource["BrandInformation"]</h6>
        </div><!-- slim-pageheader -->

 <div class="section-wrapper">
         @* <label class="section-title">Please fill your company information</label>*@
          @*<p class="mg-b-20 mg-sm-b-40">Please fill your company information</p>*@

          <div class="form-layout">  
            <div class="row mg-b-25">
              <div class="col-lg-4">
                  <label>@sharedResource["BrandTitle"]: <span class="tx-danger">*</span></label>
                  <div style="font-size:xx-large">@Model.Brand.BrandTitle</div>
              </div><!-- col-4 -->

            </div><!-- row -->

            <div class="form-layout-footer">
                 <form method="post" asp-route-id="@Model.Brand.BrandId" >
                   <input type="submit" value='@sharedResource["Delete"]' class="btn btn-danger bd-0">
                       <a  asp-area="Admin" asp-page="/BrandManagment/BrandList">
                 <input type='button' class="btn btn-secondary bd-0" value='@sharedResource["Cancel"]' />
              </a>
                  </form>
           
            </div><!-- form-layout-footer -->
       
          </div><!-- form-layout -->
        </div><!-- section-wrapper -->
        
      </div><!-- container -->
    </div>

    
@section Scripts {
 <script>

 
    </script>
}